<?xml version="1.0" encoding="utf-8" ?>
<!-- test configuration edtFTPnet nunit tests -->
<configuration>
  <appSettings>
  	<add key="edtftp.log.level" value="ALL"/>
  	<add key="ftptest.host" value="buzzard"/>
  	<add key="ftptest.user" value="javaftp"/>
  	<add key="ftptest.password" value="javaftp"/>
  	<add key="ftptest.timeout" value="1"/>
  	<add key="ftptest.strictreplies" value="true"/>
  	<add key="ftptest.connectmode" value="ACTIVE"/>
  	<add key="ftptest.testdir" value="remote/test"/>
  	<add key="ftptest.file.local.text" value="test1.txt"/>
  	<add key="ftptest.file.remote.text" value="test1.txt"/>
  	<add key="ftptest.file.local.binary" value="test.dll"/>
  	<add key="ftptest.file.remote.binary" value="remote.dll"/>
  	<add key="ftptest.file.local.empty" value="empty_local.txt"/>
  	<add key="ftptest.file.remote.empty" value="empty_remote.txt"/>
  	<add key="ftptest.dir.remote.empty" value="empty"/>	
  </appSettings>
</configuration>
