<?xml version="1.0"?>
<project name="ftp client" default="build">
	<target name="build">
	
		<csc target="library" debug="true" output="ftpclient.dll">
			<sources>
				<includes name="*.cs"/>
			</sources>
		</csc>
		
	</target>
	
	<target name="clean">
		<delete file="ftpclient.dll" failonerror="false" />
		<delete file="ftpclient.pdb" failonerror="false" />
	</target>
</project>